{
    "token": 20, // 0 is read in from a file (token.txt), otherwise sets the lenght of the random token
    "tokenCharset": "0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ", // pwease dwont twoutch mwe
    "writeToken" : true, // writes out the token to the console

    "port" : 12345, // sets the port of the websocket server


    "sendDataBack": true, // decides if the server sends data back to the socket where it came from
    "sendVerificationBack" : true,

    "multipleTypesInTable" : false, // if set to true you can put multiple variable types into a table
    "messageLenghtMaximum" : 256 // sets the max lenght of a message
}